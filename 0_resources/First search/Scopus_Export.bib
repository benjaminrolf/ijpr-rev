@CONFERENCE{Tran20192408,
author={Tran, K.P. and Nguyen, H.D. and Thomassey, S.},
title={Anomaly detection using Long Short Term Memory Networks and its applications in Supply Chain Management},
journal={IFAC-PapersOnLine},
year={2019},
volume={52},
number={13},
pages={2408-2412},
doi={10.1016/j.ifacol.2019.11.567},
