@ARTICLE{Wang2020,
author={Wang, Y. and Li, Q. and Guan, X. and Fan, J. and Liu, Y. and Wang, H.},
title={Collaboration and resource sharing in the multidepot multiperiod vehicle routing problem with pickups and deliveries},
