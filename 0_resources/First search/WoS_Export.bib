DOI = {10.3390/en14206682},
Article-Number = {6682},
Author-Email = {falhamel@uwaterloo.ca
   ali.ahmadian@uwaterloo.ca
   aelkamel@uwaterloo.ca},
Times-Cited = {3},
Unique-ID = {WOS:000714667600001},
DA = {2023-02-21},
}

