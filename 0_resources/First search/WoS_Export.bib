DOI = {10.1108/SCM-06-2012-0209},
Author-Email = {emel.aktas@brunel.ac.uk},
Times-Cited = {17},
Unique-ID = {WOS:000322942600005},
DA = {2023-02-21},
}

@article{ WOS:000487997100153,
