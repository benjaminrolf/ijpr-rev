@article{ WOS:000605460600001,
Author = {Gambella, Claudio and Ghaddar, Bissan and Naoum-Sawaya, Joe},
Title = {Optimization problems for machine learning: A survey},
Journal = {EUROPEAN JOURNAL OF OPERATIONAL RESEARCH},
