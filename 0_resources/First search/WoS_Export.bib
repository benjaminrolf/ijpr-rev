DOI = {10.1007/s10479-015-1853-1},
Author-Email = {brandenburg@uni-kassel.de
   tobias.rebs@tu-berlin.de},
Times-Cited = {125},
Unique-ID = {WOS:000354387600009},
DA = {2023-02-21},
}

