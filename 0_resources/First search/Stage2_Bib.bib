Qi, G. Y., E China Univ Sci & Technol, Sch Business, Shanghai 200237, Peoples R China.},
 author = {Xie, X. M. and Zang, Z. P. and Qi, G. Y.},
 author-email = {xxm@shu.edu.cn},
 da = {2023-02-21},
 doi = {10.1016/j.jclepro.2015.08.006},
 journal = {JOURNAL OF CLEANER PRODUCTION},
