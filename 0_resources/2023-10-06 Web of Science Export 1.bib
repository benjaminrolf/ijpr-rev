@article{ WOS:000771506200001,
Author = {Naz, Farheen and Agrawal, Rohit and Kumar, Anil and Gunasekaran, Angappa
   and Majumdar, Abhijit and Luthra, Sunil},
Title = {Reviewing the applications of artificial intelligence in sustainable
   supply chains: Exploring research propositions for future directions},
Journal = {BUSINESS STRATEGY AND THE ENVIRONMENT},
Year = {2022},
