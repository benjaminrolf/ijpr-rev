@article{ WOS:000254207200020,
Author = {Li, Guo-Dong and Yamaguchi, Daisuke and Nagai, Masatake},
Title = {A grey-based rough decision-making approach to supplier selection},
Journal = {INTERNATIONAL JOURNAL OF ADVANCED MANUFACTURING TECHNOLOGY},
