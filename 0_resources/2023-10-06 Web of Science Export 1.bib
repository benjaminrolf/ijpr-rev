@article{ WOS:000669016700007,
Author = {Qu, Qiao and Liu, Cheng and Bao, Xinzhong},
Title = {E-Commerce Enterprise Supply Chain Financing Risk Assessment Based on
   Linked Data Mining and Edge Computing},
