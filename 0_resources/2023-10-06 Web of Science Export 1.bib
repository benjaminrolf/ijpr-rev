@article{ WOS:000548024200001,
Author = {Abdirad, Maryam and Krishnan, Krishna},
Title = {Industry 4.0 in Logistics and Supply Chain Management: A Systematic
   Literature Review},
Journal = {ENGINEERING MANAGEMENT JOURNAL},
