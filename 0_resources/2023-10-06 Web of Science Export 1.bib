@article{ WOS:000664938900005,
Author = {Bakhtadze, Natalia and Suleykin, Alexander},
Title = {Industrial digital ecosystems: Predictive models and architecture
   development issues},
Journal = {ANNUAL REVIEWS IN CONTROL},
Year = {2021},
Volume = {51},
Pages = {56-64},
Abstract = {The concept of digital ecosystem (DES) is widely used in autonomous
   manufacturing process control and the development of complex, stable,
   interactive, self-organizing and reliable management systems for various
   industries. The paper offers a concept of DES control system
   architecture based on predictive models. For estimating and predicting
   the state of resources in production processes, an approach is developed
   using data mining based model generation. The paper also reviews the
   current state of research in the field of DES and their applications in
   supply chain management (SCM).},
