@article{ WOS:000668683200008,
Author = {Strakova, Jarmila and Simberova, Iveta and Partlova, Petra and Vachal,
   Jan and Zich, Robert},
Title = {The Value Chain as the Basis of Business Model Design},
Journal = {JOURNAL OF COMPETITIVENESS},
