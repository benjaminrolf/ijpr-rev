@article{ WOS:000572625500022,
Author = {Zhang, Xin and Du, Ke-Jing and Zhan, Zhi-Hui and Kwong, Sam and Gu,
   Tian-Long and Zhang, Jun},
Title = {Cooperative Coevolutionary Bare-Bones Particle Swarm Optimization With
   Function Independent Decomposition for Large-Scale Supply Chain Network
   Design With Uncertainties},
