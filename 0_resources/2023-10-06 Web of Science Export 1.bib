@article{ WOS:000987794900001,
Author = {Gurcan, Fatih and Boztas, Gizem Dilan and Dalveren, Gonca Gokce Menekse
   and Derawi, Mohammad},
Title = {Digital Transformation Strategies, Practices, and Trends: A Large-Scale
   Retrospective Study Based on Machine Learning},
Journal = {SUSTAINABILITY},
