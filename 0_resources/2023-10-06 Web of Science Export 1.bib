@article{ WOS:000757521600001,
Author = {Xia, Wei and Wang, Shi and Shi, Mingjun and Xia, Qing and Jin, Wenting},
Title = {Research on partition strategy of urban water supply network based on
   optimized hierarchical clustering algorithm},
Journal = {WATER SUPPLY},
Year = {2022},
