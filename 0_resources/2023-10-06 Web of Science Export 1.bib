@article{ WOS:000253183700018,
Author = {Liao, Shu-Hsien and Chen, Chyuan-Meei and Wu, Chung-Hsin},
Title = {Mining customer knowledge for product line and brand extension in
   retailing},
Journal = {EXPERT SYSTEMS WITH APPLICATIONS},
Year = {2008},
Volume = {34},
