@article{ WOS:000802958800007,
Author = {Guan, Zhimin and Mou, Yuxia and Sun, Minghe},
Title = {Hybrid robust and stochastic optimization for a capital-constrained
   fresh product supply chain integrating risk-aversion behavior and
   financial strategies},
Journal = {COMPUTERS \& INDUSTRIAL ENGINEERING},
Year = {2022},
Volume = {169},
Month = {JUL},
Abstract = {A fresh product supply chain is considered in which an omnichannel
   retailer procures products from multiple suppliers and distributes them
   through a distribution center to physical stores to serve both online
   and offline customers. A two-stage stochastic optimization model is
   formulated for the strategic and operational decisions of the
   omnichannel retailer incorporating the decision maker's risk aversion
   behavior and financial flow into supply chain optimization. In the
   model, the amount of capital possessed by the retailer is treated as a
   hard constraint, and trade credit and bank loans are both considered as
   financial sources. After linearization, the two-stage stochastic
   optimization model is transformed into a mixed integer linear
   programming model. A hybrid robust and stochastic optimization approach
   is developed to simultaneously cope with two different types of
   uncertainties in the supply chain by including stochastic scenarios for
   transportation costs and using ``interval + polyhedral `` uncertainty
   sets for demands and deterioration rate. The robust counterpart of the
   mixed integer linear programming model is constructed. An improved
   sample average approximation method using the k means clustering
   technique is adopted to solve the problem. The applicability of the
   proposed model and the efficiency of the solution approach are
   investigated through numerical studies. Some important managerial
   insights are provided through the model, the solution approach and the
   numerical studies.},
Publisher = {PERGAMON-ELSEVIER SCIENCE LTD},
