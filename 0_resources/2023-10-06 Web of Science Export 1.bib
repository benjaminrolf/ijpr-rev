@article{ WOS:001056718300001,
Author = {Oh, Minjeong and Ahn, Chulok and Nam, Hyundong and Choi, Sungyong},
Title = {New Trends in Smart Cities: The Evolutionary Directions Using Topic
   Modeling and Network Analysis},
