@article{ WOS:000369270400001,
Author = {Xu, Nai-Ru and Liu, Jia-Bao and Li, De-Xun and Wang, Jun},
Title = {Research on Evolutionary Mechanism of Agile Supply Chain Network via
   Complex Network Theory},
