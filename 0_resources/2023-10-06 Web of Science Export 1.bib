@article{ WOS:000676918900001,
Author = {Liu, Yunmei and Zhang, Shuai and Chen, Min and Wu, Yenchun and Chen,
   Zhengxian},
Title = {The Sustainable Development of Financial Topic Detection and Trend
   Prediction by Data Mining},
Journal = {SUSTAINABILITY},
Year = {2021},
Volume = {13},
Number = {14},
Month = {JUL},
Abstract = {Blockchain technology is the most cutting-edge technology in the field
   of financial technology, which has attracted extensive attention from
   governments, financial institutions and investors of various countries.
   Blockchain and finance, as an interdisciplinary, cross-technology and
   cross-field topic, has certain limitations in both theory and
   application. Based on the bibliometrics data of Web of Science, this
   paper conducts data mining on 759 papers related to blockchain
   technology in the financial field by means of co-word analysis,
   bi-clustering algorithm and strategic coordinate analysis, so as to
   explore hot topics in this field and predict the future development
   trend. The experimental results found ten research topics in the field
   of blockchain combined with finance, including blockchain crowdfunding,
   Fintech, encryption currency, consensus mechanism, the Internet of
   Things, digital financial, medical insurance, supply chain finance,
   intelligent contract and financial innovation. Among them, blockchain
   crowdfunding, Fintech, encryption currency and supply chain finance are
   the key research directions in this research field. Finally, this paper
   also analyzes the opportunities and risks of blockchain development in
   the financial field and puts forward targeted suggestions for the
   government and financial institutions.},
