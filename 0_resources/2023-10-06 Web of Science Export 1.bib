@article{ WOS:000416793400153,
Author = {Alfian, Ganjar and Syafrudin, Muhammad and Rhee, Jongtae},
Title = {Real-Time Monitoring System Using Smartphone-Based Sensors and NoSQL
   Database for Perishable Supply Chain},
