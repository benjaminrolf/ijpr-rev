@article{ WOS:000475115600001,
Author = {Li, Mei and Wu, Ying and He, Yi and Huang, Shuai and Nair, Anand},
Title = {Sparse Inverse Covariance Estimation: A Data Mining Technique to Unravel
   Holistic Patterns among Business Practices in Firms},
